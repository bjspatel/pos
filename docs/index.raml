#%RAML 1.0
title: grammar-robo
baseUri: /api
mediaType: application/json
description: REST API for the grammar-robo
/sentences:
  displayName: sentences
  '/{id}':
    displayName: '{id}'
    uriParameters:
      id:
        type: string
    get:
      displayName: GET-sentence
      responses:
        '200':
          body:
            application/json:
              type: sentence-output
              example:
                parts:
                  - content: this
                    tags:
                      - depth: '1'
                        code: ad
                        true_count: 25
                        false_count: 25
                  - content: is
                    tags:
                      - depth: '1'
                        code: vb
                        true_count: 45
                        false_count: 5
                  - content: an
                    tags:
                      - depth: '1'
                        code: ad
                        true_count: 35
                        false_count: 15
                  - content: example
                    tags:
                      - depth: '1'
                        code: nn
                        true_count: 25
                        false_count: 25
                true_percent:
                  core: 80
                  extrimity: 60
                  join: 100
                  nn: 90
                  vb: 70
                  ad: 60
                type_counts:
                  ad: 2
                  nn: 1
                  vb: 1
                appeared: 50
                core-level: 1
                extrimism-level: 1
                join-level: 0
                vocab-level: 1
    put:
      displayName: PUT-sentence
      body:
        application/json:
          type: sentence-input
          example:
